{
  "manifest_version": 3,
  "name": "webtube-ai",
  "version": "1.2.4",
  "description": "webtube-ai",
  "default_locale": "en",
  "icons": {
    "128": "img/icon-128.png"
  },
  "author": "aditya anand",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "options_page": "options.html",
  "permissions": ["activeTab", "scripting", "storage"]
}
